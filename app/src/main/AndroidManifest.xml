<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.adolf.adolf">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".intent.StartIntentActivity"></activity>

        <provider
            android:name=".wsp3.AdolfDbProvider"
            android:authorities="cn.adolf.db.provider"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".wsp3.DbMainActivity" />
        <activity android:name=".wsp2.WorkspaceActivity" />
        <activity
            android:name=".wsp1.RvAdvanceActivity"
            android:label="Adolf中自身应用的">
            <intent-filter>
                <action android:name="666" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>